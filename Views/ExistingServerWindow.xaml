<Window x:Class="ChatRoom.Views.ExistingServerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChatRoom.Views"
        mc:Ignorable="d"
        Title="Choose a server to run" Height="120" Width="300">
    <StackPanel>
        <Label Content="Choose which server to use:" HorizontalAlignment="Center"/>
        <ComboBox Name="ServersBox" HorizontalAlignment="Center" MinWidth="145"/>
        <Button Content="Ok" MinWidth="60" HorizontalAlignment="Center" Margin="0 10 0 0" Click="Button_Click"/>
    </StackPanel>
</Window>
